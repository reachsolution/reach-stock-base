{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch server.js",
        "program": "${workspaceRoot}/src/server.js",
        "outFiles": [ "${workspaceRoot}/.compiled/**/*.js" ]
      },
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Current File",
        "program": "${file}",
        "outFiles": [ "${workspaceRoot}/.compiled/**/*.js" ]
      }
    ]
  }